.tianxiedizhi {
    width: 686rpx;
    padding: 0 42rpx;
    margin: 12rpx auto 0;
    box-sizing: border-box;
    background: #fff;
    box-shadow: 0 3px 24px 0 hsla(0,0%,63.9%,.2);
    border-radius: 10px;
}

.row {
    display: flex;
    border-bottom: 1rpx solid #e8e8e8;
    min-height: 100rpx;
    max-height: 300rpx;
    height: auto;
    font-size: 28rpx;
    color: #000;
    line-height: 100rpx;
    justify-content: space-between;
    align-items: center;
    padding-left: 64rpx;
}

.row:last-child {
    border-bottom: none;
    padding-left: 0;
}

.row input {
    height: 100rpx;
    line-height: 100rpx;
    width: 420rpx;
}

.name {
    background: url(data:image/png;base64,) 0 no-repeat;
}

.phone {
    background: url(data:image/png;base64,) 0 no-repeat;
}

.city {
    background: url(data:image/png;base64,) 0 no-repeat;
}

.detailAddress {
    background: url(data:image/png;base64,) 0 no-repeat;
}

.detailAddress textarea {
    line-height: 38rpx;
    max-height: 300rpx;
}

.city,.detailAddress,.name,.phone {
    background-size: 32rpx 32rpx;
}

.btnsure {
    padding: 14rpx 0;
    width: 686rpx;
    border-radius: 10rpx;
    background: #fff;
    box-shadow: 0 3px 24px 0 hsla(0,0%,63.9%,.2);
    color: #ff7a00;
    line-height: 60rpx;
    text-align: center;
    position: fixed;
    bottom: 14rpx;
    left: 50%;
    transform: translateX(-50%);
}

.btnsure.addbottom {
    bottom: 62rpx;
}

.input {
    padding-top: 300rpx;
    width: 100%;
}

.input input {
    width: 100%;
    background-color: #fff;
    border-bottom: 1px solid #d9d9d9;
    border-top: 1px solid #d9d9d9;
    padding: 20rpx 50rpx;
}

.section {
    padding-top: 20rpx;
}

.userinfo-avatar {
    width: 128rpx;
    height: 128rpx;
    margin: 20rpx;
    border-radius: 50%;
}

.userinfo-nickname {
    color: #aaa;
}

.usermotto {
    margin-top: 200px;
}

.modal {
    position: fixed;
    top: 0;
    left: 0;
    background: rgba(0,0,0,.3);
    z-index: 1;
}

.container,.modal {
    height: 100%;
    width: 100%;
}

.container {
    position: absolute;
    min-height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    padding: 0;
    box-sizing: border-box;
    background-color: #f8f8f8;
}

.citypicker {
    position: fixed;
    bottom: 0;
    z-index: 3;
    background: #fff;
    font-size: 28rpx;
    text-align: center;
    transform: translateY(150%);
    border-radius: 4rpx 4rpx 0 0;
    height: 300px;
    width: 100%;
}

.cityheader {
    width: 100%;
    background: #fff;
    border-radius: 4rpx 4rpx 0 0;
    height: 80rpx;
}

.city-cancel {
    float: left;
    margin: 20rpx;
    color: #818181;
}

.city-true {
    float: right;
    margin: 20rpx;
    color: #2fb42e;
}

.section .picker {
    background-color: #fff;
    border-bottom: 1px solid #d9d9d9;
    border-top: 1px solid #d9d9d9;
    padding: 20rpx;
}